package TransactionRules
import com.sample.TransactionDetails;
import com.sample.AccountDetails;
dialect "java"
salience 30

rule "pin-validity"
lock-on-active
    when
        atransaction : AccountDetails(p : pin);
        transaction : TransactionDetails(pin != p);
    then
        transaction.setFScore(100);
       update(transaction)
		System.out.println("Invalid Pin Name");

end