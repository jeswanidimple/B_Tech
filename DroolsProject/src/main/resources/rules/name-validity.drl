package TransactionRules
import com.sample.TransactionDetails;
import com.sample.AccountDetails;

dialect "java"
salience 45

rule "name-validity"
lock-on-active
    when
        atransaction : AccountDetails(n : name);
        transaction : TransactionDetails(name != n);
    then
        transaction.setFScore(100);
       update(transaction)
		System.out.println("Invalid Card Name");

end

